{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
      "builder": "NIXPACKS"
    },
    "deploy": {
      "startCommand": "mvn spring-boot:run -Dserver.address=0.0.0.0",
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 10
    },
    "env": {
      "SERVER__ADDRESS": "0.0.0.0"
    }
  }